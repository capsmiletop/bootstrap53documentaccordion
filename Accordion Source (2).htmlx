    <style>
        /* Override Bootstrap accordion styles to match source */
        #root {
            background-color: #000;
            color: #fff;
            padding-top: 5rem;
        }
        
        .accordion {
            --bs-accordion-bg: transparent;
            --bs-accordion-border-color: transparent;
        }
        
        .accordion-item {
            background-color: transparent;
            border: none;
            margin-bottom: 1.25rem;
        }
        
        .accordion-button {
            background-color: transparent;
            color: #fff;
            border: 4px solid #10b981;
            border-radius: 1.5rem;
            padding: 1.75rem;
            font-size: 1rem;
            font-weight: 500;
            text-align: left;
            position: relative;
            box-shadow: none;
        }
        
        .accordion-button:not(.collapsed) {
            background-color: rgba(16, 185, 129, 0.1);
            color: #fff;
        }
        
        .accordion-button:focus {
            box-shadow: none;
            border-color: #10b981;
        }
        
        .accordion-button::after {
            display: none;
        }
        
        .accordion-button.collapsed::before {
            content: '+';
            position: absolute;
            right: 1rem;
            top: 50%;
            transform: translateY(-50%);
            font-size: 2rem;
            font-weight: 300;
            line-height: 1;
        }
        
        .accordion-button:not(.collapsed)::before {
            content: '+';
            position: absolute;
            right: 1rem;
            top: 50%;
            transform: translateY(-50%) rotate(45deg);
            font-size: 2rem;
            font-weight: 300;
            line-height: 1;
        }
        
        .accordion-body {
            padding: 0;
            background-color: transparent;
        }
        
        .accordion-body ul {
            list-style: none;
            padding: 0;
            margin: 0;
        }
        
        .accordion-body ul li {
            margin-bottom: 0.5rem;
        }
        
        .accordion-body a,
        .accordion-body .document-link {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 1.75rem;
            padding-right: 1rem;
            border: 4px solid #10b981;
            border-radius: 1.5rem;
            color: #fff;
            text-decoration: none;
            font-size: 1rem;
            transition: background-color 0.2s;
        }
        
        .accordion-body a:hover,
        .accordion-body .document-link:hover {
            background-color: rgba(16, 185, 129, 0.1);
        }
        
        .accordion-body a span,
        .accordion-body .document-link span {
            flex: 1;
        }
        
        .accordion-body a .icon-wrapper,
        .accordion-body .document-link .icon-wrapper {
            display: flex;
            align-items: center;
            flex-shrink: 0;
        }
        
        .accordion-body a img,
        .accordion-body .document-link img {
            width: 2rem;
            height: 2rem;
        }
        
        .accordion-button .button-content {
            display: flex;
            align-items: center;
            justify-content: space-between;
            width: 100%;
        }
        
        .accordion-button .button-content span {
            flex: 1;
        }
        
        .accordion-button .button-content .icon-wrapper {
            display: flex;
            align-items: center;
            flex-shrink: 0;
            margin-left: 1rem;
        }
        
        .accordion-button .button-content .icon-wrapper .parent-image {
            display: none;
        }
        
        @media (min-width: 768px) {
            .accordion-button {
                font-size: 1.25rem;
                padding-top: 0;
                padding-bottom: 0;
            }
            
            .accordion-button .button-content .icon-wrapper .parent-image {
                display: block;
                width: 7rem;
                height: 7rem;
            }
            
            .accordion-body a,
            .accordion-body .document-link {
                font-size: 1.25rem;
                padding-top: 0;
                padding-bottom: 0;
            }
        }
        
        @media (min-width: 1024px) {
            .accordion-button {
                padding: 1.75rem;
            }
            
            .accordion-body a,
            .accordion-body .document-link {
                padding: 1.75rem;
                padding-right: 1rem;
            }
        }
        
        section {
            margin: 1.5rem 0;
            padding: 0 1rem;
        }
        
        @media (min-width: 768px) {
            section {
                padding: 0 3rem;
            }
        }
        
        h1 {
            color: #10b981;
            font-weight: 500;
            text-align: center;
            font-size: 1.875rem;
            margin: 0;
            text-decoration: underline;
        }
        
        @media (min-width: 768px) {
            h1 {
                font-size: 3rem;
            }
        }
    </style>
</head>
